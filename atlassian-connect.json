{
  "key": "confluence-byline-addon",
  "name": "Byline Item for Confluence",
  "description": "Page Approvals Integration",
  "vendor": {
    "name": "Confluence Tutorials",
    "url": "https://www.atlassian.com/"
  },
  "baseUrl": "{{localBaseUrl}}",
  "links": {
    "self": "{{localBaseUrl}}/atlassian-connect.json",
    "homepage": "{{localBaseUrl}}/atlassian-connect.json"
  },
  "translations": {
    "paths": {
      "en-US": "https://dji84rix1yofm.cloudfront.net/i18n/en.json",
      "en-UK": "https://dji84rix1yofm.cloudfront.net/i18n/en.json"
    }
  },
  "authentication": {
    "type": "jwt"
  },
  "lifecycle": {
    "installed": "/installed"
  },
  "scopes": [
    "READ",
    "WRITE"
  ],
  "modules": {
    "contentBylineItems": [
      {
        "context": "addon",
        "target": {
          "type": "inlinedialog"
        },
        "tooltip": {
          "value": "Page Approval"
        },
        "name": {
          "value": "Page Approval",
          "i18n": "byline.default"
        },
        "key": "byline-item",
        "url": "/approvals?contentId={content.id}",
        "contentPropertyKey": "byline-ui"
      }
    ]
  },
  "apiMigrations": {
        "gdpr": true
    }
}